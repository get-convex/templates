    "@tanstack/react-start": "^1.121.38",
    "convex": "^1.27.0",
    "react-dom": "^19.1.0",
    "react": "^19.1.0"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.11",
    "@types/react-dom": "^19.1.6",
    "@types/react": "^19.1.8",
    "@vitejs/plugin-react": "^4.6.0",
    "concurrently": "~9.2.0",
    "prettier": "^3.6.1",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.8.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vite": "^6.3.5"
